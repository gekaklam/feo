<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

  <!-- vertex property keys -->
  <key id="name" for="node" attr.name="name" attr.type="string" />
  <key id="eval" for="node" attr.name="eval" attr.type="string" />

  <!-- edge property keys -->
  <key id="pos" for="node" attr.name="pos" attr.type="vector_float" />
  <key id="weight" for="edge" attr.name="weight" attr.type="double" />

  <graph id="G" edgedefault="directed">

   <!-- graph properties -->

   <!-- vertices -->
   <node id="n00">
	   <data key="name">Client:A</data>
	   <data key="eval">Client.Client(s)</data>
   </node>
   <node id="n01">
	   <data key="name">Client:B</data>
   </node>
   <node id="n1">
	   <data key="name">Switch</data>
   </node>
   <node id="n2">
	   <data key="name">IOServer</data>
   </node>
   <node id="n3">
	   <data key="name">Cache</data>
   </node>
   <node id="n4">
	   <data key="name">Switch</data>
   </node>
   <node id="n5">
	   <data key="name">Disk Cache</data>
	   <data key="eval">Cache.Cache(s)</data>
   </node>
   <node id="n6">
	   <data key="name">Drive:A</data>
   </node>
   <node id="n7">
	   <data key="name">Drive:B</data>
   </node>
   <node id="n8">
	   <data key="name">Library</data>
	   <data key="eval">Library.Library(s, 5,5)</data>
   </node>

   <!-- edges -->
    <edge id="e0" source="n00" target="n1">
		<data key="weight">10</data>
    </edge>
    <edge id="e1" source="n01" target="n1">
		<data key="weight">10</data>
    </edge>
    <edge id="e2" source="n2" target="n1">
		<data key="weight">30.0</data>
    </edge>
    <edge id="e3" source="n2" target="n3">
		<data key="weight">20.0</data>
    </edge>
    <edge id="e4" source="n2" target="n4">
		<data key="weight">30.0</data>
    </edge>
    <edge id="e5" source="n5" target="n4">
      <data key="weight">20.0</data>
    </edge>
    <edge id="e6" source="n6" target="n4">
      <data key="weight">5.0</data>
    </edge>
    <edge id="e7" source="n7" target="n4">
      <data key="weight">5.0</data>
    </edge>

  </graph>
</graphml>
